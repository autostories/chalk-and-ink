{
  // https://code.visualstudio.com/api/extension-guides/color-theme
  "colors": {
    "activityBar.activeBorder": "#1878d8",
    "activityBar.background": "#151615",
    "activityBar.border": "#2a2b2a",
    "activityBar.foreground": "#d6d8da",
    "activityBar.inactiveForeground": "#737475",
    "editor.background": "#1a1b1b",
    "editor.lineHighlightBorder": "#0000",
    "editor.lineHighlightBackground": "#141313",
    "editorLineNumber.activeForeground": "#3f9ede",
    "editorLineNumber.foreground": "#575a5c",
    "foreground": "#b8bbbe",
    "minimap.background": "#151615",
    "panel.background": "#151615",
    "panel.border": "#2a2b2a",
    "panelTitle.activeBorder": "#0b78e5",
    "panelTitle.activeForeground": "#d6d8da",
    "panelTitle.inactiveForeground": "#737475",
    "sideBar.background": "#151615",
    "sideBar.border": "#2a2b2a",
    "sideBarSectionHeader.background": "#151615",
    "sideBarSectionHeader.border": "#2a2b2a",
    "statusBar.background": "#151615",
    "statusBar.border": "#2a2b2a",
    "statusBar.foreground": "#b9bcc0",
    "statusBarItem.remoteBackground": "#1b6db5",
    "statusBarItem.remoteForeground": "#ffffff",
    "titleBar.activeBackground": "#151615",
    "titleBar.border": "#2a2b2a",
    "titleBar.inactiveBackground": "#161716"
  },
  "semanticHighlighting": true,
  "semanticTokenColors": {},
  "tokenColors": [
    {
      // Dim comments punctuations.
      "scope": [
        "punctuation.definition.comment.html",
        "punctuation.definition.comment.json",
        "punctuation.definition.comment.ini",
        "punctuation.definition.comment.toml",
        "punctuation.definition.comment.xml",
        "punctuation.definition.comment.yaml"
      ],
      "settings": { "foreground": "#504a4a", "fontStyle": "" }
    },
    {
      // Inline and block comments.
      "scope": [
        "comment.line.dotenv",
        "comment.block.html",
        "comment.line.double-slash.js",
        "comment.block.json.comments",
        "comment.line.number-sign.toml",
        "comment.block.xml",
        "comment.line.number-sign.yaml"
      ],
      "settings": { "foreground": "#62696a", "fontStyle": "italic" }
    },
    {
      // Dim punctuations.
      "scope": [
        "punctuation.definition.string.begin.html",
        "punctuation.definition.string.end.html",
        "punctuation.definition.string.begin.js",
        "punctuation.definition.string.end.js",
        "punctuation.definition.template-expression.begin.js",
        "punctuation.definition.template-expression.end.js",
        "punctuation.support.type.property-name.begin.json",
        "punctuation.support.type.property-name.end.json",
        "punctuation.definition.string.begin.json",
        "punctuation.definition.string.end.json",
        "punctuation.definition.string.begin.xml",
        "punctuation.definition.string.end.xml",
        "punctuation.separator.namespace.xml",
        "punctuation.definition.string.begin.yaml",
        "punctuation.definition.string.end.yaml"
      ],
      "settings": { "foreground": "#7c7272", "fontStyle": "" }
    },
    {
      // Punctuations.
      "scope": [
        "punctuation.section.property-list.begin.bracket.curly.css",
        "punctuation.section.property-list.end.bracket.curly.css",
        "punctuation.separator.key-value.css",
        "punctuation.terminator.rule.css",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.definition.block.js",
        "punctuation.definition.parameters.begin.js",
        "punctuation.definition.parameters.end.js",
        "punctuation.definition.dictionary.begin.json",
        "punctuation.definition.dictionary.end.json",
        "punctuation.definition.array.begin.json",
        "punctuation.definition.array.end.json",
        "punctuation.separator.dictionary.pair.json",
        "punctuation.separator.dictionary.key-value.json",
        "punctuation.separator.array.json",
        "punctuation.definition.table.toml",
        "punctuation.definition.array.table.toml",
        "punctuation.definition.array.toml",
        "punctuation.definition.table.inline.toml",
        "punctuation.separator.array.toml",
        "punctuation.separator.table.inline.toml",
        "punctuation.definition.tag.xml",
        "punctuation.separator.key-value.mapping.yaml",
        "punctuation.definition.sequence.begin.yaml",
        "punctuation.definition.sequence.end.yaml",
        "punctuation.separator.sequence.yaml",
        "punctuation.definition.mapping.begin.yaml",
        "punctuation.definition.mapping.end.yaml",
        "punctuation.separator.mapping.yaml",
        "punctuation.definition.block.sequence.item.yaml"
      ],
      "settings": { "foreground": "#a8b6a6", "fontStyle": "" }
    },
    {
      // Variables.
      "scope": [
        "property.value.dotenv",
        "text.html.derivative",
        "source.ignore",
        "source.ini",
        "text.xml",
        "variable.other.alias.yaml"
      ],
      "settings": { "foreground": "#b1baaf", "fontStyle": "" }
    },
    {
      // Variable definitions.
      "scope": ["entity.name.type.anchor.yaml"],
      "settings": { "foreground": "#b1baaf", "fontStyle": "underline" }
    },
    {
      // Documentation comments.
      "scope": [
        "comment.line.number-sign.ignore",
        "comment.line.number-sign.ini",
        "comment.block.documentation.json.comments"
      ],
      "settings": { "foreground": "#69825f", "fontStyle": "italic" }
    },
    {
      // Language build-in constants.
      "scope": [
        "constant.language.json",
        "storage.type.function.js",
        "storage.type.js",
        "constant.language.boolean.toml",
        "constant.language.boolean.yaml",
        "constant.language.null.yaml"
      ],
      "settings": { "foreground": "#d58080", "fontStyle": "italic" }
    },
    {
      // Special operators.
      "scope": [
        "keyword.operator.combinator.css",
        "keyword.control.flow.block-scalar.literal.yaml",
        "keyword.control.flow.block-scalar.folded.yaml",
        "storage.modifier.chomping-indicator.yaml",
        "punctuation.definition.anchor.yaml",
        "punctuation.definition.alias.yaml",
        "constant.language.merge.yaml"
      ],
      "settings": { "foreground": "#d58080", "fontStyle": "" }
    },
    {
      // Escape characters.
      "scope": [
        "constant.character.escape.json",
        "constant.character.escape.toml",
        "constant.character.escape.yaml"
      ],
      "settings": { "foreground": "#e29a48", "fontStyle": "" }
    },
    {
      // Functions.
      "scope": ["entity.name.function.js"],
      "settings": { "foreground": "#dfca61", "fontStyle": "" }
    },
    {
      // Methods.
      "scope": [],
      "settings": { "foreground": "#dfca61", "fontStyle": "italic" }
    },
    {
      // HTML and CSS tags.
      "scope": [
        "entity.name.tag.html",
        "entity.name.tag.css",
        "meta.property-name.css"
      ],
      "settings": { "foreground": "#cab136", "fontStyle": "italic" }
    },
    {
      // String.
      "scope": [
        "support.constant.property-value.css",
        "string.quoted.double.js",
        "string.quoted.single.js",
        "string.template.js",
        "string.quoted.double.json",
        "string.quoted.single.basic.line.toml",
        "string.quoted.single.literal.line.toml",
        "string.quoted.triple.basic.block.toml",
        "string.quoted.double.html",
        "string.quoted.double.xml",
        "string.unquoted.plain.out.yaml",
        "string.quoted.single.yaml",
        "string.quoted.double.yaml",
        "string.unquoted.block.yaml",
        "string.unquoted.plain.in.yaml"
      ],
      "settings": { "foreground": "#6ca375", "fontStyle": "" }
    },
    {
      // Attribute names.
      "scope": [
        "support.type.property-name.css",
        "entity.other.attribute-name.xml",
        "entity.other.attribute-name.localname.xml"
      ],
      "settings": { "foreground": "#728688", "fontStyle": "italic" }
    },
    {
      // Assign operators.
      "scope": [
        "keyword.operator.assignment.dotenv",
        "punctuation.separator.key-value.html",
        "punctuation.separator.key-value.ini",
        "keyword.operator.assignment.js",
        "punctuation.eq.toml",
        "meta.tag.preprocessor.xml"
      ],
      "settings": { "foreground": "#75b4b9", "fontStyle": "" }
    },
    {
      // Number.
      "scope": [
        "constant.numeric.css",
        "constant.numeric.json",
        "constant.numeric.integer.toml",
        "constant.numeric.float.toml",
        "constant.numeric.integer.yaml",
        "constant.numeric.float.yaml"
      ],
      "settings": { "foreground": "#32a1d4", "fontStyle": "" }
    },
    {
      // Number units.
      "scope": ["keyword.other.unit.vh.css", "keyword.other.unit.vw.css"],
      "settings": { "foreground": "#32a1d4", "fontStyle": "italic bold" }
    },
    {
      "scope": [
        "entity.name.tag.namespace.xml",
        "entity.other.attribute-name.namespace.xml"
      ],
      "settings": { "foreground": "#4996d5", "fontStyle": "italic" }
    },
    {
      // Keys and field names.
      "scope": [
        "entity.other.attribute-name.id.css",
        "variable.key.dotenv",
        "entity.other.attribute-name.html",
        "keyword.other.definition.ini",
        "support.type.property-name.json",
        "support.type.property-name.toml",
        "support.type.property-name.table.toml",
        "support.type.property-name.array.toml",
        "entity.name.tag.xml",
        "entity.name.tag.localname.xml",
        "entity.name.tag.yaml"
      ],
      "settings": { "foreground": "#9774bb", "fontStyle": "italic" }
    }
  ]
}
